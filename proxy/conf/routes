# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~
GET           /context                          controllers.Application.getBootstrapContext
POST          /context                          controllers.Application.saveBootstrapContext()
GET           /deployments                      controllers.Application.list
GET           /deployments/:deploymentId        controllers.Application.get(deploymentId: String)
POST          /deployments/:deploymentId        controllers.Application.deploy(deploymentId: String)
DELETE        /deployments/:deploymentId        controllers.Application.undeploy(deploymentId: String)
